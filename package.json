{
    "name": "root",
    "private": true,
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/fernesrou/react-fincharts.git"
    },
    "bugs": {
        "url": "https://github.com/fernesrou/react-fincharts/issues"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "scripts": {
        "bootstrap": "echo 'Bootstrap is deprecated in Lerna 7+. Using workspaces.'",
        "build": "lerna run build",
        "build:annotations": "lerna run build --scope='*/annotations'",
        "build:axes": "lerna run build --scope='*/axes'",
        "build:charts": "lerna run build --scope='react-fincharts'",
        "build:coordinates": "lerna run build --scope='*/coordinates'",
        "build:core": "lerna run build --scope='*/core'",
        "build:indicators": "lerna run build --scope='*/indicators'",
        "build:interactive": "lerna run build --scope='*/interactive'",
        "build:scales": "lerna run build --scope='*/scales'",
        "build:series": "lerna run build --scope='*/series'",
        "build:tooltip": "lerna run build --scope='*/tooltip'",
        "build:utils": "lerna run build --scope='*/utils'",
        "clean": "run-s clean:packages clean:modules",
        "clean:packages": "lerna run clean",
        "clean:modules": "lerna clean --yes",
        "convert": "echo 'Convert is deprecated. Using workspaces.'",
        "docs": "run-s build docs:stories",
        "docs:stories": "lerna run docs --stream",
        "link": "echo 'Link is deprecated. Using workspaces.'",
        "lint": "eslint . --ext ts --ext tsx --quiet",
        "lint:fix": "eslint . --ext ts --ext tsx --quiet --fix",
        "start": "run-p watch start:stories",
        "start:stories": "lerna run start --stream",
        "test": "lerna run test --stream",
        "prestart": "npm run build",
        "prepublishOnly": "npm run build",
        "publish": "lerna publish from-package --yes",
        "release": "lerna version --yes",
        "release:pre": "lerna version --conventional-prerelease --yes",
        "release:grad": "lerna version --conventional-graduate --yes",
        "watch": "lerna run watch --parallel",
        "check-react-compatibility": "node scripts/check-react-compatibility.js"
    },
    "workspaces": [
        "packages/*"
    ],
    "devDependencies": {
        "@babel/core": "^7.24.0",
        "@storybook/addon-actions": "^8.0.0",
        "@storybook/addon-docs": "^8.0.0",
        "@storybook/addon-essentials": "^8.0.0",
        "@storybook/react": "^8.0.0",
        "@storybook/react-vite": "^8.0.0",
        "@storybook/theming": "^8.0.0",
        "@types/d3-array": "^3.2.1",
        "@types/d3-dsv": "^3.0.7",
        "@types/d3-force": "^3.0.10",
        "@types/d3-format": "^3.0.4",
        "@types/d3-interpolate": "^3.0.4",
        "@types/d3-path": "^3.1.0",
        "@types/d3-scale": "^4.0.8",
        "@types/d3-selection": "^3.0.10",
        "@types/d3-shape": "^3.1.6",
        "@types/d3-time-format": "^4.0.3",
        "@types/lodash.flattendeep": "^4.4.9",
        "@types/react": "^18.2.79",
        "@types/react-dom": "^18.2.25",
        "@typescript-eslint/eslint-plugin": "^7.7.0",
        "@typescript-eslint/parser": "^7.7.0",
        "babel-loader": "^9.1.3",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.34.1",
        "find-cache-dir": "^4.0.0",
        "lerna": "^8.1.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.2.5",
        "react-docgen-typescript-loader": "^3.7.2",
        "rimraf": "^5.0.5",
        "source-map-loader": "^5.0.0",
        "storybook": "^8.0.0",
        "ts-loader": "^9.5.1",
        "typescript": "^5.4.5"
    },
    "dependencies": {
        "@react-fincharts/annotations": "file:packages/annotations",
        "@react-fincharts/axes": "file:packages/axes",
        "@react-fincharts/coordinates": "file:packages/coordinates",
        "@react-fincharts/core": "file:packages/core",
        "@react-fincharts/indicators": "file:packages/indicators",
        "@react-fincharts/interactive": "file:packages/interactive",
        "@react-fincharts/scales": "file:packages/scales",
        "@react-fincharts/series": "file:packages/series",
        "@react-fincharts/stories": "file:packages/stories",
        "@react-fincharts/tooltip": "file:packages/tooltip",
        "@react-fincharts/utils": "file:packages/utils",
        "react-fincharts": "file:packages/charts"
    }
}
